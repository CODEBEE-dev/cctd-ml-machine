{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/__tests__/*.test.ts"
    ],
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup_tests.ts",
      "jest-expect-message"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.3.3",
    "@smui/dialog": "^7.0.0-beta.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.2.3",
    "@types/js-cookie": "^3.0.2",
    "@types/web-bluetooth": "^0.0.16",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.29.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-expect-message": "^1.1.3",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.7.2",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-windicss-preprocess": "^4.1.6-beta.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^2.8.4",
    "@sgratzl/chartjs-chart-boxplot": "^3.5.1",
    "@types/w3c-web-usb": "^1.0.6",
    "bowser": "^2.11.0",
    "chart.js": "^3.9.1",
    "chartjs-chart-pcp": "^3.5.0",
    "dapjs": "^2.3.0",
    "js-cookie": "^3.0.1",
    "ml5": "^0.7.2",
    "postcss": "^8.4.18",
    "sirv-cli": "^1.0.0",
    "svelte-skeleton": "^1.3.1",
    "uuid4": "^2.0.2"
  }
}
